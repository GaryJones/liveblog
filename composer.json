{
	"name": "automattic/liveblog",
	"type": "wordpress-plugin",
	"description": "Liveblogging done right. Using WordPress.",
	"keywords": [
		"liveblog",
		"live event coverage",
		"plugin",
		"wordpress"
	],
	"homepage": "https://github.com/Automattic/liveblog",
	"license": "GPL-2.0-or-later",
	"require": {
		"php": ">=5.6",
		"composer/installers": "^1.0",
		"predis/predis": "^1.0.3",
		"rase/socket.io-emitter": "^0.7.0"
	},
	"require-dev": {
		"automattic/vipwpcs": "^0.3.0",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
		"phpcompatibility/phpcompatibility-wp": "^2",
		"squizlabs/php_codesniffer": "^3.3.2",
		"wp-coding-standards/wpcs": "^1.1.0"
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"install-codestandards": "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run",
		"phpcs": "phpcs && @phpcs-vip-minimum",
		"phpcs-vip-go": "phpcs . --standard=WordPress-VIP-Go -spn --ignore=assets,bin,node_modules,vendor --extensions=php",
		"phpcs-vip-minimum": "phpcs . --standard=WordPressVIPMinimum -spn --ignore=assets,bin,node_modules,vendor --extensions=php",
		"phpcs-phpcompatibility": "phpcs . --standard=PHPCompatibility -sp --ignore=assets,bin,node_modules,vendor --extensions=php --runtime-set testVersion 5.6",
		"test": "phpunit"
	},
	"support": {
		"issues": "https://github.com/Automattic/liveblog/issues",
		"source": "https://github.com/Automattic/liveblog"
	}
}
